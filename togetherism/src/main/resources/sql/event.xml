<?xml version="1.0" encoding="UTF-8" ?>
 <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="eventns">

<insert id="create" parameterType="event">
insert into event values(event_seq.nextval,#{club_num},#{club_host_email},#{event_date},
sysdate,#{event_date},#{event_info},#{event_file},#{event_spot_long},#{event_spot_lat})
</insert>
</mapper>






















